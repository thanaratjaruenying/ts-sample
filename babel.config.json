{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    ["@babel/preset-typescript"],
    ["@babel/preset-flow"],
    ["@babel/preset-react"]
  ],
  "plugins": [
    ["@babel/plugin-transform-flow-strip-types"],
    ["const-enum"],
    ["@babel/plugin-transform-typescript"],
    ["@babel/plugin-syntax-dynamic-import"],
    ["@loadable/babel-plugin"]
  ]
}
